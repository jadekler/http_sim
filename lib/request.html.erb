<html>
<head></head>
<body>
<h2>Requests for: <%= endpoint.method %> <%= endpoint.path %></h2>

<table>
  <tbody>
  <% endpoint.requests.each_with_index do |request, index| %>
  <tr>
    <td><%= index %>:</td>
    <td><pre><%= request %></pre></td>
  </tr>
  <% end %>
  </tbody>
</table>
</body>
</html>
